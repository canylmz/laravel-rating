{
    "name": "canylmz/laravel-rating",
    "description": "Associate ratings to any Eloquent model",
    "license": "MIT",
    "authors": [
        {
            "name": "Can YILMAZ",
            "email": "canylmz6134@gmail.con"
        }
    ],
    "homepage": "https://github.com/canylmz/laravel-rating",
    "keywords": [
        "eloquent",
        "laravel",
        "laravel-rating",
        "model",
        "rating",
        "rate",
        "stars",
        "star",
        "system"
    ],
    "require": {
        "illuminate/support": "5.*|6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Compubel\\Rating\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Compubel\\Rating\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Canylmz\\Rating\\RatingServiceProvider"
            ]
        }
    }
}
