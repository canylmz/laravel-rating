{
    "name": "canylmz/laravel-rating",
    "version": "v0.0.1",
    "description": "Associate ratings to any Eloquent model",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Can YILMAZ",
            "email": "canylmz6134@gmail.con"
        }
    ],
    "homepage": "https://github.com/canylmz/laravel-rating",
    "keywords": [
        "eloquent",
        "laravel",
        "laravel-rating",
        "model",
        "rating",
        "rate",
        "stars",
        "star",
        "system"
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "6.*",
        "laravel/helpers": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "8.3.*",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^4.0",
        "sempro/phpunit-pretty-print": "1.1.*"
    },
    "autoload": {
        "psr-4": {
            "Canylmz\\Rating\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Canylmz\\Rating\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Canylmz\\Rating\\RatingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
